{
  "mcpServers": {
    "playwright": {
      "command": "npx",
      "args": ["@playwright/mcp@latest"]
    }
  },
  "permissions": {
    "allow": [
      "Bash(npm:*)",
      "Bash(node:*)",
      "Bash(python3:*)",
      "Bash(npx:*)",
      "mcp__playwright__*",
      "Read(*)",
      "Write(~/job-hunter/*)"
    ]
  }
}
